{
  "name": "metadata-submitter-frontend",
  "version": "0.13.0",
  "private": true,
  "dependencies": {
    "@apidevtools/json-schema-ref-parser": "^9.0.9",
    "@emotion/react": "^11.8.2",
    "@emotion/styled": "^11.8.1",
    "@mui/codemod": "^5.5.3",
    "@mui/icons-material": "^5.5.1",
    "@mui/lab": "^5.0.0-alpha.75",
    "@mui/material": "^5.5.3",
    "@mui/styles": "^5.5.3",
    "@mui/system": "^5.5.2",
    "@mui/x-data-grid": "^5.7.0",
    "@reduxjs/toolkit": "^1.8.0",
    "@testing-library/jest-dom": "^5.16.3",
    "@testing-library/react": "^12.1.4",
    "@types/webpack-env": "^1.16.3",
    "@typescript-eslint/parser": "^5.17.0",
    "ajv": "^8.11.0",
    "ajv-formats": "^2.1.1",
    "ajv-i18n": "^4.2.0",
    "apisauce": "^2.1.5",
    "i18next": "^21.6.14",
    "jest": "26.6.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-dropzone": "^12.0.4",
    "react-hook-form": "^7.28.1",
    "react-i18next": "^11.16.2",
    "react-redux": "^7.2.7",
    "react-router-dom": "^6.3.0",
    "react-scripts": "^4.0.3"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "test:noninteractive": "react-scripts test --watchAll=false --env=jsdom",
    "lint": "eslint --fix .",
    "lint:check": "eslint .",
    "format": "prettier --write \"**/*.+(json|yml|yaml|css|md)\"",
    "format:check": "prettier --check \"**/*.+(json|yml|yaml|css|md)\"",
    "concurrently": "concurrently",
    "postinstall": "husky install",
    "tsc": "tsc --pretty"
  },
  "eslintConfig": {
    "env": {
      "cypress/globals": true
    },
    "extends": [
      "react-app",
      "react-app/jest",
      "plugin:prettier/recommended",
      "plugin:cypress/recommended"
    ],
    "plugins": [
      "cypress"
    ],
    "rules": {
      "additional-rule": "warn"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/user-event": "^13.5.0",
    "@types/lodash": "^4.14.180",
    "@types/node": "^17.0.23",
    "@types/react-dom": "^17.0.14",
    "@types/react-redux": "^7.1.23",
    "@types/react-router-dom": "^5.3.3",
    "@types/redux-mock-store": "^1.0.3",
    "@typescript-eslint/eslint-plugin": "^5.17.0",
    "chokidar": "^3.5.3",
    "concurrently": "^7.0.0",
    "cypress": "^9.5.3",
    "cypress-file-upload": "^5.0.8",
    "eslint": "7.32.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jest": "^26.1.3",
    "eslint-plugin-prettier": "^4.0.0",
    "http-proxy-middleware": "^2.0.4",
    "husky": "^7.0.4",
    "jest": "26.6.0",
    "mongo": "^0.1.0",
    "msw": "^0.39.2",
    "pinst": "^3.0.0",
    "prettier": "^2.6.1",
    "redux-mock-store": "^1.5.4",
    "snapshot-diff": "^0.9.0",
    "typescript": "^4.6.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run concurrently -- \"npm:lint:check\" \"npm:format:check\" \"npm:tsc\" \"npm:test:noninteractive\" "
    }
  }
}
