{
  "name": "metadata-submitter-frontend",
  "version": "0.13.0",
  "private": true,
  "dependencies": {
    "@apidevtools/json-schema-ref-parser": "^11.1.0",
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "@mui/codemod": "^5.15.8",
    "@mui/icons-material": "^5.15.8",
    "@mui/lab": "^5.0.0-alpha.164",
    "@mui/material": "^5.15.7",
    "@mui/styles": "^5.15.8",
    "@mui/system": "^5.15.8",
    "@mui/x-data-grid": "^6.19.3",
    "@reduxjs/toolkit": "^2.1.0",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^12.1.5",
    "@types/webpack-env": "^1.18.4",
    "@typescript-eslint/parser": "^5.23.0",
    "ajv": "^8.12.0",
    "ajv-formats": "^2.1.1",
    "ajv-i18n": "^4.2.0",
    "apisauce": "3.0.0",
    "i18next": "^21.10.0",
    "jest": "26.6.0",
    "lodash": "^4.17.21",
    "moment": "^2.30.1",
    "path-browserify": "^1.0.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-dropzone": "^12.1.0",
    "react-hook-form": "^7.50.1",
    "react-i18next": "^11.17.2",
    "react-redux": "^8.0.0",
    "react-router-dom": "^6.22.0",
    "react-scripts": "^5.0.1",
    "react-transition-group": "^4.4.2"
  },
  "scripts": {
    "start": "GENERATE_SOURCEMAP=false react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test --env=jsdom",
    "test:noninteractive": "react-app-rewired test --watchAll=false --env=jsdom",
    "lint": "eslint --fix .",
    "lint:check": "eslint .",
    "format": "prettier --write \"**/*.+(json|yml|yaml|css|md)\"",
    "format:check": "prettier --check \"**/*.+(json|yml|yaml|css|md)\"",
    "concurrently": "concurrently",
    "postinstall": "husky install",
    "tsc": "tsc --pretty"
  },
  "eslintConfig": {
    "env": {
      "cypress/globals": true
    },
    "extends": [
      "react-app",
      "react-app/jest",
      "plugin:prettier/recommended",
      "plugin:cypress/recommended"
    ],
    "plugins": [
      "cypress"
    ],
    "rules": {
      "additional-rule": "warn"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@testing-library/user-event": "^14.0.4",
    "@types/lodash": "^4.14.202",
    "@types/node": "^18.0.0",
    "@types/react-dom": "^17.0.14",
    "@types/react-router-dom": "^5.3.3",
    "@types/redux-mock-store": "^1.0.6",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "chokidar": "^3.5.3",
    "concurrently": "^7.6.0",
    "cypress": "^13.6.4",
    "cypress-file-upload": "^5.0.8",
    "eslint": "7.32.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-cypress": "^2.15.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jest": "^26.1.3",
    "eslint-plugin-prettier": "^4.2.1",
    "http-proxy-middleware": "^2.0.6",
    "husky": "^7.0.4",
    "jest": "26.6.0",
    "mongo": "^0.1.0",
    "msw": "^0.49.0",
    "pinst": "^3.0.0",
    "prettier": "^2.8.8",
    "react-app-rewired": "^2.2.1",
    "redux-mock-store": "^1.5.4",
    "snapshot-diff": "^0.10.0",
    "typescript": "^4.9.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run concurrently -- \"npm:lint:check\" \"npm:format:check\" \"npm:tsc\" \"npm:test:noninteractive\" "
    }
  }
}
