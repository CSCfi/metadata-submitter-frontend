{
  "name": "metadata-submitter-frontend",
  "version": "0.9.2",
  "private": true,
  "dependencies": {
    "@apidevtools/json-schema-ref-parser": "^9.0.7",
    "@material-ui/core": "^4.11.3",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.57",
    "@reduxjs/toolkit": "^1.5.1",
    "@testing-library/jest-dom": "^5.11.10",
    "@testing-library/react": "^11.2.6",
    "ajv": "^8.0.5",
    "ajv-formats": "^2.0.2",
    "apisauce": "^2.0.1",
    "jest-environment-jsdom-sixteen": "^1.0.3",
    "lodash": "^4.17.21",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-hook-form": "^6.15.5",
    "react-redux": "^7.2.3",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^4.0.3"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jest-environment-jsdom-sixteen",
    "test:noninteractive": "react-scripts test --watchAll=false --env=jest-environment-jsdom-sixteen",
    "lint": "eslint --fix .",
    "lint:check": "eslint .",
    "format": "prettier --write \"**/*.+(json|yml|yaml|css|md)\"",
    "format:check": "prettier --check \"**/*.+(json|yml|yaml|css|md)\"",
    "flow:check": "flow check",
    "concurrently": "concurrently",
    "postinstall": "husky install"
  },
  "eslintConfig": {
    "env": {
      "cypress/globals": true
    },
    "extends": [
      "react-app",
      "plugin:prettier/recommended",
      "plugin:cypress/recommended"
    ],
    "plugins": [
      "cypress"
    ],
    "rules": {
      "additional-rule": "warn"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/user-event": "^13.1.1",
    "chokidar": "^3.5.1",
    "concurrently": "^6.0.0",
    "cypress": "^6.8.0",
    "cypress-file-upload": "^5.0.3",
    "eslint": "7.23.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-cypress": "^2.11.2",
    "eslint-plugin-flowtype": "^5.7.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.3.4",
    "eslint-plugin-prettier": "^3.3.1",
    "flow-bin": "^0.148.0",
    "http-proxy-middleware": "^1.1.0",
    "husky": "^6.0.0",
    "jest-environment-jsdom-sixteen": "^1.0.3",
    "pinst": "^2.1.6",
    "prettier": "^2.2.1",
    "redux-mock-store": "^1.5.4",
    "snapshot-diff": "^0.8.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run concurrently -- \"npm:lint:check\" \"npm:format:check\" \"npm:flow:check\" \"npm:test:noninteractive\" "
    }
  }
}
