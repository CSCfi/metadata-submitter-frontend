{
  "name": "metadata-submitter-frontend",
  "version": "0.13.0",
  "private": true,
  "dependencies": {
    "@apidevtools/json-schema-ref-parser": "^11.5.4",
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "@mui/codemod": "^5.15.14",
    "@mui/icons-material": "^5.15.14",
    "@mui/lab": "^5.0.0-alpha.169",
    "@mui/material": "^5.15.14",
    "@mui/system": "^5.15.9",
    "@mui/x-data-grid": "^6.19.5",
    "@reduxjs/toolkit": "^2.2.1",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@types/webpack-env": "^1.18.4",
    "@typescript-eslint/parser": "^7.3.1",
    "ajv": "^8.12.0",
    "ajv-formats": "^2.1.1",
    "ajv-i18n": "^4.2.0",
    "apisauce": "3.0.1",
    "browserify-fs": "^1.0.0",
    "buffer": "^6.0.3",
    "https-browserify": "^1.0.0",
    "i18next": "^23.10.1",
    "lodash": "^4.17.21",
    "moment": "^2.30.1",
    "path-browserify": "^1.0.1",
    "process": "^0.11.10",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-dropzone": "^12.1.0",
    "react-hook-form": "^7.50.1",
    "react-i18next": "^14.1.0",
    "react-redux": "^9.1.0",
    "react-router-dom": "^6.22.3",
    "react-scripts": "^5.0.1",
    "react-transition-group": "^4.4.2",
    "stream-http": "^3.2.0",
    "url": "^0.11.3",
    "util": "^0.12.5"
  },
  "scripts": {
    "start": "GENERATE_SOURCEMAP=false react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test --env=jsdom",
    "test:noninteractive": "react-app-rewired test --watchAll=false --env=jsdom",
    "lint": "eslint --fix .",
    "lint:check": "eslint .",
    "format": "prettier --write \"**/*.+(json|yml|yaml|css|md)\"",
    "format:check": "prettier --check \"**/*.+(json|yml|yaml|css|md)\"",
    "concurrently": "concurrently",
    "postinstall": "husky install",
    "tsc": "tsc --pretty"
  },
  "eslintConfig": {
    "env": {
      "cypress/globals": true
    },
    "extends": [
      "react-app",
      "react-app/jest",
      "plugin:prettier/recommended",
      "plugin:cypress/recommended"
    ],
    "plugins": [
      "cypress"
    ],
    "rules": {
      "additional-rule": "warn"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@testing-library/user-event": "^14.0.4",
    "@types/jest": "^29.5.12",
    "@types/lodash": "^4.17.0",
    "@types/node": "^20.11.30",
    "@types/react-dom": "^18.2.22",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^7.3.1",
    "chokidar": "^3.5.3",
    "concurrently": "^7.6.0",
    "cypress": "^13.7.0",
    "cypress-file-upload": "^5.0.8",
    "eslint": "8.57.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-cypress": "^2.15.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jest": "^27.9.0",
    "eslint-plugin-prettier": "^4.2.1",
    "http-proxy-middleware": "^2.0.6",
    "husky": "^7.0.4",
    "jest": "29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "mongo": "^0.1.0",
    "msw": "^0.49.3",
    "pinst": "^3.0.0",
    "prettier": "^2.8.8",
    "react-app-rewired": "^2.2.1",
    "snapshot-diff": "^0.10.0",
    "typescript": "^4.9.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run concurrently -- \"npm:lint:check\" \"npm:format:check\" \"npm:tsc\" \"npm:test:noninteractive\" "
    }
  },
  "jest": {
    "testEnvironment": "jsdom",
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+[^esm]\\.(js|jsx|mjs|cjs|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ]
  }
}
