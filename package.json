{
  "name": "metadata-submitter-frontend",
  "version": "0.13.0",
  "type": "module",
  "private": true,
  "dependencies": {
    "@apidevtools/json-schema-ref-parser": "^11.5.4",
    "@emotion/babel-plugin": "^11.11.0",
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.5",
    "@mui/codemod": "^5.15.14",
    "@mui/icons-material": "^5.15.14",
    "@mui/lab": "^5.0.0-alpha.169",
    "@mui/material": "^5.15.14",
    "@mui/system": "^5.15.9",
    "@mui/x-data-grid": "^7.0.0",
    "@reduxjs/toolkit": "^2.2.3",
    "@typescript-eslint/parser": "^7.4.0",
    "@vitejs/plugin-react": "^4.2.1",
    "ajv": "^8.12.0",
    "ajv-formats": "^2.1.1",
    "ajv-i18n": "^4.2.0",
    "apisauce": "^3.0.1",
    "i18next": "^23.10.1",
    "lodash": "^4.17.21",
    "moment": "^2.30.1",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-dropzone": "^12.1.0",
    "react-hook-form": "^7.51.2",
    "react-i18next": "^14.1.0",
    "react-redux": "^9.1.0",
    "react-router-dom": "^6.22.3",
    "react-transition-group": "^4.4.2",
    "vite": "^5.2.7",
    "vite-tsconfig-paths": "^4.3.2"
  },
  "scripts": {
    "start": "vite",
    "build": "tsc && vite build",
    "test": "vitest watch",
    "test:no-watch": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "lint": "eslint --fix .",
    "lint:check": "eslint .",
    "format": "prettier --write \"**/*.+(json|yml|yaml|css|md)\"",
    "format:check": "prettier --check \"**/*.+(json|yml|yaml|css|md)\"",
    "concurrently": "concurrently",
    "postinstall": "husky install",
    "tsc": "tsc --pretty"
  },
  "eslintConfig": {
    "env": {
      "cypress/globals": true
    },
    "extends": [
      "react-app",
      "plugin:prettier/recommended",
      "plugin:cypress/recommended"
    ],
    "plugins": [
      "cypress"
    ],
    "rules": {
      "additional-rule": "warn"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@testing-library/user-event": "^14.0.4",
    "@types/lodash": "^4.17.0",
    "@types/node": "^20.12.2",
    "@types/react-dom": "^18.2.23",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^7.4.0",
    "@vitest/coverage-v8": "^1.4.0",
    "@vitest/ui": "^1.4.0",
    "chokidar": "^3.5.3",
    "concurrently": "^8.0.0",
    "cypress": "^13.7.1",
    "cypress-file-upload": "^5.0.8",
    "eslint": "8.57.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-cypress": "^2.15.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^7.0.4",
    "jsdom": "^24.0.0",
    "mongo": "^0.1.0",
    "msw": "^2.2.13",
    "pinst": "^3.0.0",
    "prettier": "^2.8.8",
    "snapshot-diff": "^0.10.0",
    "typescript": "^4.9.5",
    "vite-plugin-node-polyfills": "^0.21.0",
    "vitest": "^1.4.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run concurrently -- \"npm:lint:check\" \"npm:format:check\" \"npm:tsc\" \"npm:test:no-watch\" "
    }
  }
}
